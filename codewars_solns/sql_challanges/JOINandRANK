--https://www.codewars.com/kata/58094559c47d323ebd000035
SELECT PEOPLE.ID,
	PEOPLE.NAME,
	COUNT(SALES.SALE) AS SALE_COUNT,
	RANK () OVER (
															ORDER BY COUNT(SALES.SALE) DESC) SALE_RANK
FROM exercise1.PEOPLE,
	exercise1.SALES
WHERE PEOPLE.ID = SALES.PEOPLE_ID
GROUP BY PEOPLE.ID,
	PEOPLE.NAME
ORDER BY COUNT(SALES.SALE) DESC